			"unitMiND.memUnitVersionMajor": 1,
			"unitMiND.memKnowledgeTags": {
				"unitMiND.tagUtype": "unitBrain.tagUtypeModule"
			}
		},
		
		"localStoreFolder": {
			"unitMiND.memKnowledgeType": "unitMiND.typVar",
			"unitMiND.memMemberVarValue": "../../Store"
		},
		"appStreamRoot": {
			"unitMiND.memKnowledgeType": "unitStream.typStream",
			"unitStream.memStreamPath": "localStoreFolder"
		},
		"appStreamProvider": {
			"unitMiND.memKnowledgeType": "unitMiND.typAgent",
			"unitMiND.memAgentLogic": "unitStream.logStreamProvider",
			"unitStream.memStreamProviderLocalRoot": "appStreamRoot",
			"unitStream.memStreamProviderContentRoots": {
				"unitMiND.typUnit" : "Units",
				"unitBrain.typModule" : "Impl/Java/lib"
			}
		},
		
		"appJson": {
			"unitMiND.memKnowledgeType": "unitMiND.typAgent",
			"unitMiND.memAgentLogic": "unitBrainJava.logStreamSerializerJson",
			"unitStream.memStreamSerializerCondition":"*.json"
		},
		"appStreamPortal": {
			"unitMiND.memKnowledgeType": "unitMiND.typAgent",
			"unitMiND.memAgentLogic": "unitStream.logStreamPortal",
			"unitStream.memStreamPortalMediators": [
				"appJson"
			]
		},
		
		"appMsg": {
			"unitMiND.memKnowledgeType": "unitText.typText",
			"unitText.memTextString": "Hello, World!"
		},
		"appLog": {
			"unitMiND.memKnowledgeType": "unitMiND.typAgent",
			"unitMiND.memAgentLogic": "unitBrainJava.logStreamSerializerTextLine",
			"unitStream.memStreamSerializerStream": "streamOut"
		},
		"lsnrLog": {
			"unitMiND.memKnowledgeType": "unitMiND.typListener",
			"unitMiND.memListenerTarget": "message",
			"unitMiND.memListenerHandler": [
				"appLog"
			],
			"unitMiND.memKnowledgeTags": {
				"unitMiND.tagListener": "unitMiND.tagListenerProcessor"
			}
		},
		
		"unitMain": {
			"unitMiND.memKnowledgeType": "unitMiND.typUnit",
			"message": "appMsg"
		},
		
		"dlgMain": {
			"unitMiND.memKnowledgeType": "unitMiND.typAgent",
			"unitMiND.memAgentLogic": "unitMiND.logDialog",
			"unitMiND.memDialogUnit": "unitMain",
			"unitMiND.memOrchestratorAgents": [
				"appLog"
			],
			"unitMiND.memDialogListeners": [
				"lsnrLog"
			],
			"unitMiND.memDialogLaunch": [
				"message"
			]
		},
				
		"app": {
			"unitMiND.memKnowledgeType": "unitBrain.typApp",
			"unitMiND.memUnitAuthor": "authDust",
			"unitMiND.memKnowledgeIdentifier": "HelloWorld",
			"unitMiND.memUnitVersionMajor": 1,
			"unitBrain.memAppStreamProvider" : "appStreamProvider",
			"unitBrain.memAppStreamPortal": "appStreamPortal",
			"unitBrain.memAppApi" : "modApi",
			"unitBrain.memAppModules": [
				"modBrainJava"
			],
			"unitBrain.memAppMain": "dlgMain"
		},
				
		"theBrain" : {
			"unitBrain.memBrainDialogs": [
				"dlgMain"
			]
		},
		

		"_End_": "____________"

	}
