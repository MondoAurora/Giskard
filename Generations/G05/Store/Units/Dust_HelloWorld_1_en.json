			"unitMiND.memUnitVersionMajor": 1,
			"unitMiND.memKnowledgeTags": {
				"unitMiND.tagUtype": "unitMiND.tagUtypeUnit"
			},
			"unitMiND.memKnowledgeRequires": [
				"unitMiND", "unitStream", "unitText", "unitBrain", "unitBrainJava"
			]
		},
		
		"modApi": {
			"unitMiND.memKnowledgeType": "unitBrain.typModule",
